    "def solution(s):\n",
    "    stack = []\n",
    "    for i in s:\n",
    "        if i == '(':\n",
    "            stack.append(i)\n",
    "        if i == ')':\n",
    "            if stack != []:\n",
    "                del stack[-1]\n",
    "            else:\n",
    "                return False\n",
    "    return True\n",
    "solution(\"()()\")"
   ]
