    "def solution(n):\n",
    "    c = 1\n",
    "    while n != 1:\n",
    "        if n % 2 == 1:\n",
    "            c += 1\n",
    "        n = n//2\n",
    "\n",
    "        \n",
    "    return c\n",
    "\n",
    "solution(5000)"
