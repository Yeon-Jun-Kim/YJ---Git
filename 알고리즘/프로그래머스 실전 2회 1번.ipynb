    "import itertools\n",
    "def solution(number):\n",
    "    answer = itertools.combinations(number, 3)\n",
    "    answer = list(answer)\n",
    "    for i in range(len(answer)):\n",
    "        answer[i] = sum(answer[i])\n",
    "    return answer.count(0)\n",
    "solution([-3, -2, -1, 0, 1, 2, 3])"
   ]
